#. type: Plain text
#: en/git-replace.txt:118
#, priority:100
msgid "<replaced sha1>"
msgstr ""

#. type: Plain text
#: en/git-replace.txt:119
#, fuzzy, priority:100
msgid "'medium':"
msgstr "'medium'"

#. type: Plain text
#: en/git-replace.txt:120
#, priority:100
msgid "<replaced sha1> -> <replacement sha1>"
