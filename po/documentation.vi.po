msgstr "git-stage(1)"

#. type: Plain text
#: en/git-stage.txt:7
#, fuzzy, priority:100
msgid "git-stage - Add file contents to the staging area"
msgstr "Thêm nội dung tập tin vào vùng bệ phóng"

#. type: Plain text
#: en/git-stage.txt:13
#, ignore-ellipsis, no-wrap, priority:100
msgid "'git stage' <arg>...\n"
msgstr ""

#. type: Plain text
#: en/git-stage.txt:20
#, priority:100
